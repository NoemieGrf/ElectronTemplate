{
  "compilerOptions": {
    "target": "es2022",
    "module": "CommonJS",
    "lib": [
      "dom",
      "esnext"
    ],
    "strict": true,
    "pretty": true,
    "sourceMap": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "outDir": "dist"
  },
  "include": ["src/**/*",]
}